# Database
PSQL_HOST=localhost
PSQL_DATABASE=yavai
PSQL_PORT=5432
PSQL_USER=postgres
PSQL_PASSWORD=password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Spark
SPARK_HOME=/opt/spark
SPARK_JARS=/opt/spark/jars/*
SPARK_JARS_AWS=org.apache.hadoop:hadoop-aws:3.3.1
SPARK_JARS_TFRECORD=org.tensorflow:spark-tensorflow-connector_2.12:1.15.0
SPARK_JARS_EXCEL=com.crealytics:spark-excel_2.12:0.13.5
SPARK_HADOOP_HIVE_METASTORE_URIS=thrift://localhost:9083
